{
  "name": "@react-native-vector-icons/matrialcommunityicons",
  "description": "Material Community Icons font for react native vector icons",
  "version": "0.0.1",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/src/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "dependencies": {
    "@react-native-vector-icons/common": "^0"
  },
  "devDependencies": {
    "@mdi/font": "^6.5.95",
    "del-cli": "^5.1.0",
    "react-native-builder-bob": "^0.23.1",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "scripts": {
    "clean": "del-cli android/build ios/build lib",
    "prepare": "bob build",
    "update": "scripts/update.sh"
  },
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "*.podspec",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "keywords": [
    "react-native",
    "ios",
    "android",
    "osx",
    "windows",
    "macos",
    "react-component",
    "react-native-component",
    "react",
    "mobile",
    "ui",
    "icon",
    "icons",
    "vector",
    "retina",
    "font",
    "materialcommunityicons"
  ],
  "author": {
    "name": "Joel Arvidsson",
    "email": "joel@oblador.se"
  },
  "homepage": "https://github.com/oblador/react-native-vector-icons",
  "bugs": {
    "url": "https://github.com/oblador/react-native-vector-icons/issues"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/oblador/react-native-vector-icons.git"
  },
  "license": "MIT",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "engines": {
    "node": ">= 18.0.0"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.json"
        }
      ]
    ]
  },
  "packageManager": "yarn@4.0.1"
}
