{
  "generator-react-native-vector-icons": {
    "packageName": "ant-design",
    "packageJSON": {
      "devDependencies": {
        "@ant-design/icons-svg": "^4.3.1"
      }
    },
    "buildSteps": {
      "fixSVGPaths": {
        "location": "../../node_modules/@ant-design/icons-svg/inline-namespaced-svg/outlined"
      },
      "fontCustom": {
        "location": "fixedSvg",
        "cleanup": true
      },
      "glyphmap": {
        "mode": "css",
        "cleanup": true
      }
    }
  }
}
