{
  "generator-react-native-vector-icons": {
    "packageName": "ant-design",
    "upstreamFont": "@ant-design/icons-svg",
    "versionSuffix": "-alpha",
    "buildSteps": {
      "fixSVGPaths": {
        "location": "../../node_modules/@ant-design/icons-svg/inline-namespaced-svg/outlined"
      },
      "fontCustom": {
        "location": "fixedSvg",
        "cleanup": true
      },
      "glyphmap": {
        "mode": "css",
        "cleanup": true
      }
    }
  }
}
