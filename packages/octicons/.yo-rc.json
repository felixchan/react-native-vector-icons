{
  "generator-react-native-vector-icons": {
    "packageName": "octicons",
    "upstreamFont": "@primer/octicons",
    "versionSuffix": "-alpha",
    "buildSteps": {
      "fixSVGPaths": {
        "location": "../../node_modules/@primer/octicons/build/svg",
        "keepPostfix": "-16"
      },
      "fontCustom": {
        "location": "fixedSvg",
        "cleanup": true
      },
      "glyphmap": {
        "mode": "css",
        "cleanup": true
      }
    }
  }
}
