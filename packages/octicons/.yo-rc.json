{
  "generator-react-native-vector-icons": {
    "packageName": "octicons",
    "packageJSON": {
      "devDependencies": {
        "@primer/octicons": "^19.8.0"
      }
    },
    "buildSteps": {
      "fixSVGPaths": {
        "location": "../../node_modules/@primer/octicons/build/svg",
        "keepPostfix": "-16"
      },
      "fontCustom": {
        "location": "fixedSvg",
        "cleanup": true
      },
      "glyphmap": {
        "mode": "css",
        "cleanup": true
      }
    }
  }
}
