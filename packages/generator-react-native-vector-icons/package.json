{
  "name": "generator-react-native-vector-icons",
  "version": "0.1.0",
  "description": "Generates React Native vector icons font library",
  "type": "module",
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "react-native-vector-icons",
    "yeoman-generator"
  ],
  "dependencies": {
    "yeoman-generator": "^1.0.0"
  },
  "devDependencies": {
    "@types/copyfiles": "^2",
    "@types/yeoman-generator": "^5.2.14",
    "copyfiles": "^2.4.1",
    "onchange": "^7.1.0",
    "tsc-watch": "^6.0.4",
    "typescript": "^5.3.2"
  },
  "scripts": {
    "build": "tsc && npm run copydeps",
    "copydeps": "copyfiles --up 1 'src/*/templates/**' generators",
    "lint": "eslint ./src --ext .js,.ts",
    "clean": "rm -rf ./generators",
    "watch": "npm run watch-tsc --silent & npm run watch-deps --silent",
    "watch-deps": "onchange 'src/*/templates/**' --initial -- npm run copydeps",
    "watch-tsc": "tsc -w"
  }
}
